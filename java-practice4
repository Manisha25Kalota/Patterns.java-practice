import java.util.*;

public class Patterns {
    public static void main(String args[]){
        int n = 50;
        int m = 100;

        //outer loop
        for(int i=1; i<=n; i++){
            //inner loop
            for(int j=1; j<=m; j++){
                System.out.print("*");
            }
            System.out.println();
        }
        

    }
}
